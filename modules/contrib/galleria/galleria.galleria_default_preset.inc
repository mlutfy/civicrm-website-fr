<?php
/**
 * @file
 * Bulk export of default_galleria_presets objects generated by Bulk export
 */

/**
 * Implementation of hook_galleria_default_presets()
 */
function galleria_galleria_default_presets() {
  $presets = array();

  $preset = new stdClass;
  $preset->disabled = FALSE; /* Edit this to true to make a default preset disabled initially */
  $preset->api_version = 1;
  $preset->name = 'default';
  $preset->title = 'Default';
  $preset->theme = 'classic';
  $preset->imagestyle_thumb = 'galleria_thumb';
  $preset->imagestyle_normal = 'galleria_zoom';
  $preset->imagestyle_big = '';
  $preset->options = array(
    'height' => 300,
    'width' => 450,
  );
  $presets['default'] = $preset;

  return $presets;
}
